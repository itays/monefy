{
  "name": "monefy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:client": "cd client && yarn start",
    "serve": "concurrently \"webpack -w\" \"nodemon --inspect dist/App.bundle.js\"",
    "start:server": "nodemon dist/App.bundle.js",
    "heroku-postbuild": "yarn build && cd client && yarn --production=false && yarn build",
    "start:dev": "concurrently --kill-others-on-fail \"yarn serve\" \"yarn start:client\"",
    "start": "node dist/App.bundle.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^5.0.1",
    "express": "~4.16.0",
    "express-validator": "^5.1.2",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.12",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/express-validator": "^3.0.0",
    "@types/joi": "^13.0.7",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/mongoose": "^5.0.10",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.60",
    "@types/passport": "^0.4.4",
    "@types/passport-jwt": "^3.0.1",
    "@types/validator": "^9.4.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.4",
    "ts-loader": "^4.2.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-node-externals": "^1.7.2"
  }
}
